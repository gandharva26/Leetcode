class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        c=0
        for i in range(0,len(s)):
            x=''
            tc=0
            for j in range(i,len(s)):
                if(s[j] in x):
                    break
                x+=s[j]
                tc+=1
            if(tc==len(s)):
                return tc
            if(tc>c):
                c=tc
           
        return c
